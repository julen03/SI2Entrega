<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<managed-bean>
		<managed-bean-name>createRideBean1</managed-bean-name>
		<managed-bean-class>beans.CreateRideBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>findRidesBean</managed-bean-name>
		<managed-bean-class>beans.FindRidesBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>datarekinBeans</managed-bean-name>
		<managed-bean-class>beans.DatarekinBeans</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>Prueba.xhtml</display-name>
		<from-view-id>/Prueba.xhtml</from-view-id>
		<navigation-case>
			<to-view-id>/CreateRide.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Prueba.xhtml</display-name>
		<from-view-id>/Prueba.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>queryBuelta</from-outcome>
			<to-view-id>/QueryRides.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>QueryRides.xhtml</display-name>
		<from-view-id>/QueryRides.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>query</from-outcome>
			<to-view-id>/Prueba.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>CreateRide.xhtml</display-name>
		<from-view-id>/CreateRide.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>create</from-outcome>
			<to-view-id>/Prueba.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Prueba.xhtml</display-name>
		<from-view-id>/Prueba.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>DataAukeratu</from-outcome>
			<to-view-id>/DataBatenBidaiak.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>DataBatenBidaiak.xhtml</display-name>
		<from-view-id>/DataBatenBidaiak.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>BidaiakErakutsi</from-outcome>
			<to-view-id>/BidaiakErakutsi.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
